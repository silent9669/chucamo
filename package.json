{
  "name": "bluebook-sat-simulator",
  "version": "1.0.0",
  "description": "A comprehensive SAT preparation platform with practice tests, analytics, and study tools",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "build:prod": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "start": "node start.js",
    "server:prod": "node server/index.js",
    "postinstall": "cd client && npm install",
    "heroku-postbuild": "npm run build:prod",
    "create-admin": "node server/scripts/createAdmin.js",
    "migrate-articles": "node server/scripts/migrateArticles.js",
    "remove-sample-articles": "node server/scripts/removeSampleArticles.js",
    "update-user-streak-fields": "node server/scripts/updateUserStreakFields.js"
  },
  "keywords": [
    "sat",
    "education",
    "test-prep",
    "react",
    "nodejs",
    "mongodb"
  ],
  "author": "Bluebook SAT Simulator Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "mongoose-paginate-v2": "^1.9.1",
    "multer": "^1.4.5-lts.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  }
}
